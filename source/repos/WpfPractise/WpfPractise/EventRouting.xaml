<Window x:Class="WpfPractise.EventRouting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPractise"
        mc:Ignorable="d"
        Title="EventRouting" Height="300" Width="300">
    <Grid>
        <Border Margin="20" BorderBrush="Coral" BorderThickness="15" Padding="10" x:Name="border">
            <StackPanel Background="Gainsboro" x:Name="panel" Orientation="Vertical">
                <Rectangle x:Name="rectangle" Fill="Aqua" Margin="20" Height="80"></Rectangle>
               <Ellipse x:Name="ellipse" Fill="DarkKhaki" Margin="0" Height="30"></Ellipse>
            </StackPanel>
        </Border>
        
        
    </Grid>
</Window>
