<Window x:Class="WpfPractise.TreeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPractise"
        mc:Ignorable="d"
        Title="TreeView" Height="300" Width="300">
    <Grid>
        <TreeView>
            <TreeViewItem Header="Departments">
                <TreeViewItem Header="CSE"></TreeViewItem>
                <TreeViewItem Header="ECE"></TreeViewItem>
                <TreeViewItem Header="EEE"></TreeViewItem>
                <TreeViewItem Header="MECH"></TreeViewItem>
            </TreeViewItem>

        </TreeView>
    </Grid>
</Window>
